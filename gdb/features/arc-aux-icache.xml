<!DOCTYPE feature SYSTEM "gdb-target.dtd">
<feature name="org.gnu.gdb.arc.aux-icache">
  <!-- ARC instruction cache auxiliary register set -->
  <reg name="ic_ivic" bitsize="32"/>
  <reg name="ic_ctrl" bitsize="32"/>
  <reg name="ic_lil" bitsize="32"/>
  <reg name="ic_ivil" bitsize="32"/>
  <reg name="ic_ram_address" bitsize="32"/>
  <reg name="ic_tag" bitsize="32"/>
  <reg name="ic_data" bitsize="32"/>
</feature>
