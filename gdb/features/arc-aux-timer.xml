<!DOCTYPE feature SYSTEM "gdb-target.dtd">
<feature name="org.gnu.gdb.arc.aux-timer">
  <!-- ARC timer auxiliary register set -->
  <reg name="count0" bitsize="32"/>
  <reg name="control0" bitsize="32"/>
  <reg name="limit0" bitsize="32"/>
  <reg name="count1" bitsize="32"/>
  <reg name="control1" bitsize="32"/>
  <reg name="limit1" bitsize="32"/>
  <reg name="aux_rtc_ctrl" bitsize="32"/>
  <reg name="aux_rtc_low" bitsize="32"/>
  <reg name="aux_rtc_high" bitsize="32"/>
</feature>
