<!DOCTYPE feature SYSTEM "gdb-target.dtd">
<feature name="org.gnu.gdb.arc.aux-dcache">
  <!-- ARC data cache auxiliary register set -->
  <reg name="dc_ivdc" bitsize="32"/>
  <reg name="dc_ctrl" bitsize="32"/>
  <reg name="dc_flsh" bitsize="32"/>
  <reg name="aux_cache_limit" bitsize="32"/>
  <reg name="dc_ldl" bitsize="32"/>
  <reg name="dc_ivdl" bitsize="32"/>
  <reg name="dc_fldl" bitsize="32"/>
  <reg name="dc_ram_addr" bitsize="32"/>
  <reg name="dc_tag" bitsize="32"/>
  <reg name="dc_data" bitsize="32"/>
</feature>
